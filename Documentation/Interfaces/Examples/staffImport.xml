	<!-- staffMember ==> Required:  externalId, firstName, lastName, positionType, department
	                     Optional:  middleName, email, acadTitle
	-->
<staff campus="PWL" term="Fal" year="2010">
	<!--
		Since UniTime 3.3.165 positionCode attribute (that has to be translated through position_code_to_type table) was replaced by positionType attribute
		that refers directly to position_type table.
		Attribute acadTitle (academic title) was added in UniTime 3.5.172
	 -->
	<staffMember externalId="100" firstName="Joe" middleName="S" lastName="Doe" acadTitle="Dr." positionType="ASSOC_PROF" department="0101" email="doe@unitime.org"/>
	<staffMember externalId="101" firstName="George" lastName="Newman" acadTitle="Prof." positionType="PROF" department="0101"/>
	<staffMember externalId="102" firstName="John" middleName="William" lastName="Smith" acadTitle="Prof." positionType="PROF" department="0101"/>
	<staffMember externalId="103" firstName="Josef" lastName="Nowak" positionType="INSTRUCTOR" department="0101"/>
</staff>