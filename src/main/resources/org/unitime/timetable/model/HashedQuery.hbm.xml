<hibernate-mapping package="org.unitime.timetable.model">
    <class name="HashedQuery" table="hashed_queries">
        <cache include="non-lazy" usage="transactional"/>
        
        <id
            name="queryHash"
            column="query_hash"
            type="java.lang.String"
            length="48"/>
        
        <property
            name="queryText"
            column="query_text"
            type="java.lang.String"
            not-null="true"
            length="2048"/>
            
		<property
            name="created"
            column="ts_create"
            type="java.util.Date"
            not-null="true"/>
            
        <property
            name="nbrUsed"
            column="nbr_use"
            type="java.lang.Long"
            not-null="true"/>

        <property
            name="lastUsed"
            column="ts_use"
            type="java.util.Date"
            not-null="false"/>

    </class>
</hibernate-mapping>