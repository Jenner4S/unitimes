 <module>
 	<replace-with class="org.unitime.timetable.gwt.client.page.InfoPanelImpl">
 		<when-type-is class="org.unitime.timetable.gwt.client.page.InfoPanelDisplay" />
 	</replace-with>
 	<replace-with class="org.unitime.timetable.gwt.client.page.PageHeaderImpl">
 		<when-type-is class="org.unitime.timetable.gwt.client.page.PageHeaderDisplay" />
 	</replace-with>
 	<replace-with class="org.unitime.timetable.gwt.client.page.PageLabelImpl">
