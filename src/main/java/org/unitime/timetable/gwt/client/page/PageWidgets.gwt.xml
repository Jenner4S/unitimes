 		<when-type-is class="org.unitime.timetable.gwt.client.page.PageLabelDisplay.Mobile" />
 	</replace-with>
 	<replace-with class="org.unitime.timetable.gwt.client.page.UniTimeNotifications.Implementation">
 		<when-type-is class="org.unitime.timetable.gwt.client.page.UniTimeNotifications.Display" />
 	</replace-with>
 	<replace-with class="org.unitime.timetable.gwt.client.page.mobile.MobileNotifications">
 		<when-type-is class="org.unitime.timetable.gwt.client.page.UniTimeNotifications.MobileDisplay" />
 	</replace-with>
